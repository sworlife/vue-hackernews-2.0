{
  "name": "egg-vue-ssr",
  "description": "A project template that use egg and vue to implement SSR rendering",
  "version": "1.0.0",
  "author": "sworlife@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node app/web/server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config app/web/build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config app/web/build/webpack.server.config.js --progress --hide-modules",
    "postinstall": "npm run build",
    "egg-start": "egg-scripts start --daemon --title=egg-server-egg",
    "egg-stop": "egg-scripts stop --title=egg-server-egg",
    "egg-dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "egg": "^2.26.1",
    "egg-scripts": "^2.13.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "4.6.2",
    "lru-cache": "^4.1.1",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.22",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "co-request": "^1.0.0",
    "css-loader": "^0.28.7",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.15.0",
    "egg-mock": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-egg": "^8.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "iconv-lite": "^0.6.1",
    "opn": "^6.0.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^15.3.0",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
